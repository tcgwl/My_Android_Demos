    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp">

    <Button
        android:id="@+id/dagger_btn_01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dagger2基础使用"/>
    <Button
        android:id="@+id/dagger_btn_02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dagger2进阶使用"/>
    <Button
        android:id="@+id/dagger_btn_03"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Scope的使用"/>
